@{
    ViewData["Title"] = "ShowAddressReports";
}
@if (TempData["Infomessage"] != null)
{
    <div class="alert alert-danger">@TempData["Infomessage"].ToString()</div>
}
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/Reporting/Reporting/ShowReportsPage">Back to Main Reports Page</a></li>
        <li class="breadcrumb-item active" aria-current="page">Show Address Reports Page</li>
    </ol>
</nav>
<hr />
<h4>Address Reports available:</h4>
<br />
<div class="progress">
    <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100" style="width: 55%"></div>
</div>
<hr />
<p class="lead">
    <a class="btn btn-primary btn-lg" asp-area="Reporting" asp-controller="Reporting" asp-action="ShowAddressByCountry" role="button">Get addresses by country</a>
</p>
<hr />

<button type="button" class="btn btn-info">More to come ...</button>

<div>
    <p class="lead">
        <a class="btn btn-outline-link btn-lg" asp-area="" asp-controller="EGovServices" asp-action="SelectProfileSetOptions" role="button">Back to EGov Services Index </a>
    </p>
</div>

